{
  "name": "study-tool",
  "version": "0.0.0",
  "description": "make quizes for study with markdown",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src/index.js -o dist/index.js",
    "commit": "git-cz",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test": "mocha src/index.test.js -w --compilers js:babel/register",
    "test:single": "istanbul cover -x *.test.js _mocha -- -R spec src/index.test.js --compilers js:babel/register"
  },
  "keywords": [
    "UU",
    "study"
  ],
  "author": "klaaz0r",
  "license": "MIT",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "dependencies": {
    "express": "^4.15.4",
    "handlebars": "^4.0.10",
    "ramda": "^0.24.1",
    "showdown": "^1.7.4",
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "babel": "5.8.21",
    "chai": "3.2.0",
    "codecov.io": "0.1.6",
    "commitizen": "1.0.4",
    "cz-conventional-changelog": "1.1.0",
    "ghooks": "0.3.2",
    "istanbul": "0.3.18",
    "mocha": "2.2.5",
    "semantic-release": "^4.2.1"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:single && npm run check-coverage"
    }
  }
}
